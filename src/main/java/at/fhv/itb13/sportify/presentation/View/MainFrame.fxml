                  <Menu mnemonicParsing="false" text="File">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Close" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Member">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#LoadAddNewMemberView" text="Add new Member" />
                        <MenuItem mnemonicParsing="false" onAction="#LoadMemberlistView" text="Memberlist" />
                        <MenuItem mnemonicParsing="false" onAction="#LoadSearchMemberView" text="Search Member" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
         </children>
      </AnchorPane>
   </top>
